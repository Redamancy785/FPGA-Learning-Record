
#include"COMMON.h"

int main(void)
{
	//GIC 通用中断控制器初始化
	ScuGic_Init();

	//初始化 PS 端 GPIO
	PS_GPIO_Init();

	//设置 PS_KEY(MIO47)方向为输入
	PS_GPIO_SetMode(PS_KEY, INPUT, 0);

	//初始化 PS GPIO 中断
	PS_GPIO_Int_Init(PS_GPIO_IRQ_Handler);

	//设置按键 PS_KEY 的中断为边沿敏感
	PS_GPIO_SetInt(PS_KEY,XGPIOPS_IRQ_TYPE_EDGE_BOTH);


    while(1){

    }
    return 0;
}







